<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@+id/navigation_home">

    <fragment
        android:id="@+id/navigation_home"
        android:name="com.sam43.mindvalleyimageloaderapplication.ui.home.HomeFragment"
        android:label="@string/title_image_list"
        tools:layout="@layout/fragment_home" />

    <fragment
        android:id="@+id/navigation_download"
        android:name="com.sam43.mindvalleyimageloaderapplication.ui.dashboard.DashboardFragment"
        android:label="@string/title_cancel_download"
        tools:layout="@layout/fragment_dashboard" />

    <fragment
        android:id="@+id/navigation_tests"
        android:name="com.sam43.mindvalleyimageloaderapplication.ui.notifications.NotificationsFragment"
        android:label="@string/title_test"
        tools:layout="@layout/fragment_notifications" />
</navigation>